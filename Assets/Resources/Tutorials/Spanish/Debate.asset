    revealName: 0
    playerThought: 0
    triggerNiceImpression: 0
  - speakerName: 5
    speech: "Durante esa instancia, no podr\xE1s moverte: ser\xE1s forzado a demostrar
      tu poder deductivo en una intensa batalla de argumentos."
    speakerEmotion: 0
    clueInfo: {fileID: 0}
    dialogueSound:
      audioClip: {fileID: 0}
