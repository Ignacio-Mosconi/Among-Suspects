    characterEmotion: 6
    clueInfo: {fileID: 0}
    revealName: 0
    playerThought: 0
  - speakerName: 1
    speech: Get out of my sight immediately!
    characterEmotion: 4
    clueInfo: {fileID: 0}
    revealName: 0
    playerThought: 0
  interactiveConversation:
  - playerOption:
      option: Friendly
      description: '''Come on; what''s your name?'''
    dialogue:
    - speakerName: 0
      speech: Come on; let's be nice to each other. We are all trapped together here,
        aren't we?
      characterEmotion: 8
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 0
      speech: I'm Makoto Naegi. What's your name?
      characterEmotion: 8
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: Haven't you heard me before? Are you deaf?
      characterEmotion: 1
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: GET LOST!
      characterEmotion: 5
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    triggerNiceImpression: 0
  - playerOption:
      option: Challenging
      description: '''I don''t care about you...'''
    dialogue:
    - speakerName: 0
      speech: You have such a high opinion of yourself, now don't you?
      characterEmotion: 8
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 0
      speech: 'Just to be clear: I was never interested in talking to you in the first
        place.'
      characterEmotion: 8
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: Huh?
      characterEmotion: 1
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 0
      speech: I don't care about how important you think you are; your identity means
        nothing to me.
      characterEmotion: 8
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: '...'
      characterEmotion: 3
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: The liitle rat turned out to be an elocuent and interesting person after
        all...
      characterEmotion: 2
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: You need guts to talk to someone such as myself like that. I like it.
      characterEmotion: 0
      clueInfo: {fileID: 0}
      revealName: 0
      playerThought: 0
    - speakerName: 1
      speech: 'My name is <color=orange>Byakuya Togami</color>, by the way.

        I think you are someone worth to know my identity.'
      characterEmotion: 2
      clueInfo: {fileID: 0}
      revealName: 1
      playerThought: 0
    triggerNiceImpression: 1
